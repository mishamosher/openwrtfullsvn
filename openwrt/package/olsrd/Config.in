menu "olsrd - OLSR (Optimized Link State Routing) daemon"

config BR2_PACKAGE_OLSRD
	prompt "olsrd - OLSR (Optimized Link State Routing) daemon"
	tristate
	default m if CONFIG_DEVEL
	help
	  The Optimized Link State Routing daemon
	  
	  http://www.olsr.org/
	  

